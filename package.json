{
  "name": "the-josh-derocher",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --progress --config ./config/webpack.dev.js",
    "build": "webpack --progress --config ./config/webpack.prod.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "jest --coverage --env=jsdom"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread",
      "syntax-dynamic-import"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      },
      "test": {
        "plugins": [
          "babel-polyfill",
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ],
        "presets": [
          "es2015"
        ]
      }
    },
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }

{
  "name": "the-josh-derocher-react",
  "description": "The website for Josh Derocher",
  "version": "0.2.0",
  "author": "Josh Derocher <contact@thejoshderocher.com>",
  "scripts": {
    "make": "webpack --progress --config ./config/webpack.build.js --profile --json > compilation-stats.json ",
    "stats": "webpack --profile --json > compilation-stats.json --config ./config/webpack.dev.js",
    "start": "webpack-dev-server --progress --config ./config/webpack.dev.js",
    "build": "webpack -p --progress --config ./config/webpack.prod.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "jest --coverage --env=jsdom"
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-runtime",
      "transform-object-rest-spread",
      "syntax-dynamic-import"
    ],
    "env": {
      "production": {
        "presets": [
          "minify"
        ]
      },
      "test": {
        "plugins": [
          "lodash",
          "babel-polyfill",
          "transform-es2015-modules-commonjs",
          "dynamic-import-node"
        ],
        "presets": [
          "es2015"
        ]
      }
    },
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "not safari < 8",
              "not ie < 11"
            ]
          }
        }
      ]
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-code-frame": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-generator": "^6.26.0",
    "babel-helpers": "^6.24.1",
    "babel-messages": "^6.23.0",
    "babel-minify-webpack-plugin": "^0.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.9",
    "date-fns": "^1.29.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "firebase": "^4.9.0",
    "gatsby-link": "^1.6.34",
    "gatsby-plugin-create-client-paths": "^1.0.3",
    "gatsby-plugin-react-helmet": "^2.0.3",
    "gatsby-plugin-sass": "^1.0.15",
    "html-webpack-plugin": "^2.30.1",
    "immutable": "^3.8.2",
    "offline-plugin": "^4.9.0",
    "postcss-loader": "^2.0.10",
    "ramda": "^0.25.0",
    "react": "^16.2.0",
    "react-code-splitting": "^1.2.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^3.1.4",
    "react-redux": "^5.0.6",
    "react-redux-firebase": "^2.0.2",
    "react-router-dom": "^4.2.2",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-firestore": "^0.2.5",
    "standard": "^10.0.3",
    "style-loader": "^0.19.1",
    "transform-runtime": "0.0.0",
    "underscore": "^1.8.3",
    "underscore-es": "^1.9.8",
    "web-app-manifest-loader": "^0.1.1",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1",
    "webpack-split-by-path": "^2.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "lodash-webpack-plugin": "^0.11.4",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-manifest-plugin": "^1.3.2",
    "webpack-pwa-manifest": "^3.4.1"
  }
}
